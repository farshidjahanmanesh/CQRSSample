@model BookNamesDto
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>This is Sample Of CQRS in DotNet Core</p>
    <a asp-action="CreateBook" asp-controller="home">Create New Book</a>
</div>

<div>
    <ul>
        @foreach (var item in Model.Names)
        {
            <li >
                @item
            </li>
        }
    </ul>
    
</div>